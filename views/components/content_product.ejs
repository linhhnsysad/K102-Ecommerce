<div class="content">
    <div class="breadcrumb-wrapper">


        <div class="row">
            <div class="col-12">
                <!-- Recent Order Table -->
                <div class="card card-table-border-none" id="recent-orders">
                    <div class="card-header justify-content-between">
                        <h2>Recent Orders</h2>
                        <div class="date-range-report ">
                            <span></span>
                        </div>
                    </div>
                    <div class="card-body pt-0 pb-5">
                        <table class="table card-table table-responsive table-responsive-large"
                            style="width:100%">
                            <thead>
                                <tr>
                                    <th>idCatagories</th>
                                    <th>Product Name</th>
                                    <th class="d-none d-md-table-cell">Units</th>
                                    <th class="d-none d-md-table-cell">Order Date</th>
                                    <th class="d-none d-md-table-cell">Order Cost</th>
                                    <th>Status</th>
                            
                                    <th></th>
                                </tr>
                            </thead>

                            <tbody>
                                <% data.map(function(ele){ %>
                                <tr>
                                  
                                     <td><a class="text-dark"><%= ele.idCatagories %></a></td> 
                                    <td>
                                        <a class="text-dark"><%= ele.productName %></a>
                                    </td>
                                    <td class="d-none d-md-table-cell"><%= ele.productName %></td>
                                    <td class="d-none d-md-table-cell"><%= ele.productCode %></td>
                                    <td class="d-none d-md-table-cell"><%= ele.idCatagories %></td>
                                                                       
                                </tr>
                                <% }) %> 
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>